{
	"name": "c4s-api",
	"version": "0.7.2",
	"description": "Unofficial api client, types, and open-api schema for Clips4Sale adult video web store",
	"homepage": "https://github.com/dogbane-dev/c4s-api#readme",
	"bugs": {
		"url": "https://github.com/dogbane-dev/c4s-api/issues"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dogbane-dev/c4s-api.git"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"prepare": "bun simple-git-hooks",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"release": "bumpp --commit --push --tag",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"test:robust": "ROBUST_TESTS=true bun test schema-robustness.test.ts",
		"test:all": "ROBUST_TESTS=true bun test",
		"type-check": "tsc --noEmit",
		"generate:schema": "bun -e \"Bun.write('./docs/schema.json',JSON.stringify((await import('./src/open-api/schema.ts')).schema,null,2))\"",
		"generate:types": "openapi-typescript ./docs/schema.json -o ./src/client/paths.generated.ts",
		"generate": "bun run generate:schema && bun run generate:types"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@types/bun": "^1.3.4",
		"bumpp": "^10.3.2",
		"bunup": "^0.16.10",
		"openapi-typescript": "^7.10.1",
		"openapi3-ts": "^4.5.0",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.3",
		"zod-openapi": "^5.4.5"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"module": "./dist/index.js",
	"simple-git-hooks": {
		"pre-commit": "bun run lint && bun run type-check"
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@remix-run/react": "^2.17.2",
		"openapi-fetch": "^0.15.0"
	},
	"keywords": [
		"clips4sale",
		"porn",
		"fetish",
		"unofficial",
		"api",
		"videos",
		"bondage",
		"c4s"
	],
	"author": "dogbane-dev"
}
